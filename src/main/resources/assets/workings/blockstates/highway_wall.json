{
  "multipart": [
    {
      "apply": {
        "model": "workings:block/highway_wall_post",
        "uvlock": true
      }
    },
    {
      "when": {
        "north": "true"
      },
      "apply": {
        "model": "workings:block/highway_wall_side",
        "uvlock": true
      }
    },
    {
      "when": {
        "east": "true"
      },
      "apply": {
        "model": "workings:block/highway_wall_side",
        "uvlock": true,
        "y": 90
      }
    },
    {
      "when": {
        "south": "true"
      },
      "apply": {
        "model": "workings:block/highway_wall_side",
        "uvlock": true,
        "y": 180
      }
    },
    {
      "when": {
        "west": "true"
      },
      "apply": {
        "model": "workings:block/highway_wall_side",
        "uvlock": true,
        "y": 270
      }
    },
    {
      "when": {
        "flag": "1"
      },
      "apply": {
        "model": "workings:block/flag_attached"
      }
    },
    {
      "when": {
        "flag": "2"
      },
      "apply": {
        "model": "workings:block/flag_attached",
        "y": 90
      }
    }
  ]
}